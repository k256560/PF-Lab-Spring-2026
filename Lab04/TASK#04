#include <stdio.h> 
#include <string.h> 
int main() { 
    int password; 
    char username[10]; 
    printf("Enter username: "); 
    scanf("%s", username); 
    printf("Enter password: "); 
    scanf("%d", &password); 
    if (strcmp(username, "admin") == 0 && password == 1234) { 
    printf("Login Successful\n"); 
    } else { 
    printf("Invalid Credentials\n"); 
    } 
    return 0; 
} 
